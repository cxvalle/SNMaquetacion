<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SNMaquetacion.Views.Inicio"
              Visual="Material"
             Title="Maquetacion">

    <NavigationPage.TitleView >
        <StackLayout Orientation="Horizontal" HorizontalOptions="FillAndExpand">
            <Label Text="Inicio" FontSize="20"></Label>
        </StackLayout>
    </NavigationPage.TitleView>
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Taller Maquetacion"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />

            <Button Text="Mostrar Maquetacion" BackgroundColor="Blue" HorizontalOptions="FillAndExpand"  TextColor="White"
                    Clicked="Btn_Mostra_Maq">
                
            </Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>